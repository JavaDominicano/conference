                                </div>
                               
                             </div>
                            <!-- Event Schedule Tabs Content End -->

                        </div>
                        <!-- Event Schedule Tabs End -->

                    </div>
                </div>
            </div>
        </div>
        <!-- Event Schedule End -->
 <script type="module" src="js/workshops.js"></script>
 <script type="module" src="js/session.js"></script>
